program HelloWorld {
    entrypoint main = HelloWorld.main;
}

component HelloWorld {
    method main(a: int, b: int, e: int): int {
    	local c = a * 34;
		local d: int;
		switch(c + b) {
			case (20)
				return 1;
			case (-100)
				return 3;
		}
		
		return 4;
	}
	
	method fun(a: int): int {
		local b = a - 1000;
		return b - 3;  
	}
}