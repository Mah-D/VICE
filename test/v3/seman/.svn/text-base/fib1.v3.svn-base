class fib {
    field MAX: int = 200;
    field title: string = "Fibonacci program - VIRGIL\n";
    
    method main() -> int {
        local x1: int = 1,
              x2: int = 2;
        local cntr: int;

	cntr = 0;
        while ( cntr < MAX ) {
            local next: int = x1 + x2;

            x1 = x2;
            x2 = next;

            if ( cntr == MAX - 1 ) break;
            else return ();
	    cntr = cntr + 1;
        }
        return 0;
    }
}
