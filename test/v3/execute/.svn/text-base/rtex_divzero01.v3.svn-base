//@execute 0=42; 1=!DivideByZeroException; 2=!DivideByZeroException; 3=!DivideByZeroException; 4=42

component rtex_divzero01 {
    field foo: int = 0;

    method main(arg: int) -> int {
	local r = 21;
	if ( arg == 1 ) r = 1 / 0;
	if ( arg == 2 ) r = 2 / foo;
	if ( arg == 3 ) r = 3 / foo;
	return r + 21;
    }
}
